{
  "name": "common-api",
  "version": "1.0.0",
  "description": "Server side API source code",
  "repository": {},
  "author": "Worawit Boonsorn",
  "license": "ISC",
  "main": "./lib/index.js",
  "scripts": {
    "start": "node ./lib/index.js",
    "dev": "nodemon --exec babel-node ./app/index.js",
    "test": "mocha test/*.spec.js --timeout 10000 --recursive -r @babel/register --exit",
    "lint": "eslint ./app",
    "lint-test": "eslint ./test",
    "fix": "eslint ./app --fix",
    "fix-test": "eslint ./test --fix",
    "clean": "rm -rf lib && mkdir lib",
    "build-server": "babel ./app -s -d ./lib",
    "build": "npm run clean && npm run build-server"
  },
  "babel": {
    "presets": [
      "@babel/env"
    ],
    "plugins": [
      [
        "@babel/plugin-proposal-decorators",
        {
          "legacy": true
        }
      ],
      "@babel/plugin-transform-runtime"
    ]
  },
  "dependencies": {
    "@babel/runtime": "^7.11.2",
    "awilix": "^4.2.6",
    "awilix-express": "^4.0.0",
    "body-parser": "^1.19.0",
    "compression": "^1.6.2",
    "config": "^2.0.1",
    "cors": "^2.8.4",
    "express": "^4.17.1",
    "firebase-admin": "^9.1.1",
    "googleapis": "^59.0.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.20",
    "method-override": "^2.3.7",
    "moment": "^2.27.0",
    "mongoose": "^5.10.0",
    "randomatic": "^3.1.1",
    "request": "^2.88.2",
    "serve-index": "^1.9.1",
    "swagger-ui-express": "^4.1.4",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.4",
    "@babel/node": "^7.10.5",
    "@babel/plugin-proposal-decorators": "^7.10.5",
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@babel/register": "^7.10.5",
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^13.0.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^9.2.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "mocha": "^10.2.0",
    "nodemon": "^2.0.4",
    "nyc": "^15.1.0",
    "supertest": "^4.0.2"
  }
}
